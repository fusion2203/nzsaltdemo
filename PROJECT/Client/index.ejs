  <section id="shopgrid" class="shop shop-grid" style="padding: 25px 0px;overflow: hidden">
    <div class="container">
      <h1 class="text-capitalize center" style="text-align:center">Device</h1><br>
      <!-- .row end -->
      <div class="row products-container">
        <% if(devices.length > 0){ devices.forEach(function(device){ %>

        <div class="col-xs-12 col-sm-6 col-md-3 product">
          <div class="product-img">
            <img src="/<%= device.img %>" alt="Product" />
            <div class="product-hover">
              <div class="product-action">
                <!-- <a class="btn btn-primary" href="#">Add To Cart</a> -->
                <a class="btn btn-primary" href="/productDetails?main=<%= device.product_main_cat %>&page=<%= device.product_sub_cat %>&product_id=<%= device.product_id %>"
                  >Item Details</a
                >
              </div>
            </div>
          </div>
          <div class="product-bio">
            <div class="prodcut-cat">
              <a href="#"><%= device.company_name %></a>
            </div>
            <div class="prodcut-title">
              <h3>
                <a href="#"><%= device.product_name %></a>
              </h3>
            </div>
            <div class="product-price">
              <span class="symbole">â‚¹</span
              ><span><%= device.product_price %></span>
            </div>
          </div>
        </div>

        <% }); } %>
      </div>
      <!-- .row end -->
    </div>
    <!-- .container end -->
  </section>

